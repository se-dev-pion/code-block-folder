{
  "name": "code-block-folder",
  "displayName": "code-block-folder",
  "description": "A simple Visual Studio Code extension enabling customized code folding with comments to make it easier to read codes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:java",
    "onLanguage:kotlin",
    "onLanguage:dart",
    "onLanguage:swift",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:go",
    "onLanguage:python",
    "onLanguage:ruby",
    "onLanguage:shellscript",
    "onLanguage:php",
    "onLanguage:perl",
    "onLanguage:rust",
    "onLanguage:lua",
    "onLanguage:sql"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "c",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "cpp",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "csharp",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "java",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "kotlin",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "dart",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "swift",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "go",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "python",
        "path": "./snippets/sh.code-snippets"
      },
      {
        "language": "ruby",
        "path": "./snippets/sh.code-snippets"
      },
      {
        "language": "shellscript",
        "path": "./snippets/sh.code-snippets"
      },
      {
        "language": "php",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "perl",
        "path": "./snippets/sh.code-snippets"
      },
      {
        "language": "rust",
        "path": "./snippets/c.code-snippets"
      },
      {
        "language": "lua",
        "path": "./snippets/sql.code-snippets"
      },
      {
        "language": "sql",
        "path": "./snippets/sql.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}